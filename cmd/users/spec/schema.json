{
  "id": "https://raw.githubusercontent.com/benc-uk/dapr-store/master/cmd/user/spec/schema.json",
  "type": "object",
  "title": "User entity",
  "required": ["email", "userId", "displayName", "profileImage"],
  "properties": {
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "format": "email",
      "title": "The user's email"
    },
    "userId": {
      "$id": "#/properties/userId",
      "type": "string",
      "title": "The user's id"
    },
    "displayName": {
      "$id": "#/properties/displayName",
      "type": "string",
      "title": "The user's displayName"
    },
    "profileImage": {
      "$id": "#/properties/profileImage",
      "type": "string",
      "title": "User image file URL"
    }
  }
}
